{
    "buildCommand": "cd client && yarn install && ./node_modules/vite/bin/vite.js build",
    "outputDirectory": "client/dist",
    "framework": "vite",
    "routes": [
      {
        "src": "/",
        "headers": {
          "cache-control": "no-cache, no-store, must-revalidate",
          "Access-Control-Allow-Origin": "*"
        },
        "dest": "/api"
      }],
    "rewrites": [
      {
        "source": "/api/(.*)",
        "destination": "/api/index.js"
      },
      {"source": "/(.*)", "destination": "/"}
    ]
    
  }
  